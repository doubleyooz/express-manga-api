{
  "name": "backend",
  "version": "1.0.0",
  "main": "index.js",
  "type": "module",
  "author": "Waifu_Forever",
  "license": "MIT",
  "jest": {
    "transform": {},
    "globals": {
      "__DEV__": true,
      "__RCTProfileIsProfiling": false
    },    
    "setupFiles": [
      "dotenv/config"
    ],
    "setupFilesAfterEnv": [
      "./test/config/test.setup.js"
    ],
    "verbose": true
  },
  "scripts": {
    "start:dev": "NODE_ENV=dev nodemon -r dotenv/config src/index.js dotenv_config_path=./.env.dev",
    "test": "NODE_ENV=test node --experimental-vm-modules node_modules/jest/bin/jest.js --watchAll --detectOpenHandles --runInBand ./test",
    "test:e2e": "NODE_ENV=test node --experimental-vm-modules node_modules/jest/bin/jest.js --watchAll --detectOpenHandles --runInBand ./test/e2e/specs",
    "test:unit": "NODE_ENV=test node --experimental-vm-modules node_modules/jest/bin/jest.js --watchAll --detectOpenHandles --runInBand ./test/unit"
  },
  "dependencies": {
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "crypto-js": "^4.0.0",
    "date-fns": "^2.22.1",
    "date-fns-tz": "^1.1.4",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-rate-limit": "^5.4.1",
    "google-auth-library": "^7.1.2",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.11.17",
    "multer": "^1.4.2",
    "multer-gridfs-storage": "^4.2.0",
    "nodemailer": "^6.5.0",
    "path": "^0.12.7",
    "protonmail-api": "^2.1.0",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "jest": "^27.3.1",
    "mongodb-memory-server": "^7.5.1",
    "nodemon": "^2.0.7",
    "supertest": "^6.1.6"
  }
}